                <StackPanel Orientation="Vertical" Width="45">
                    <Label Content="Billete" HorizontalAlignment="Center"/>
                    <Label Content="1000" HorizontalAlignment="Center"/>
                    <Label Content="500" HorizontalAlignment="Center"/>
                    <Label Content="200" HorizontalAlignment="Center"/>
                    <Label Content="100" HorizontalAlignment="Center"/>
                    <Label Content="50" HorizontalAlignment="Center"/>
                    <Label Content="20" HorizontalAlignment="Center"/>
                    <Label Content="10" HorizontalAlignment="Center"/>
                </StackPanel>
                <StackPanel Orientation="Vertical" Width="80">
                    <Label Content="Cantidad" HorizontalAlignment="Center"/>
                    <TextBox x:Name="txtCantidad1000" Margin="0 4 0 0"/>
                    <TextBox x:Name="txtCantidad500" Margin="0 8"/>
                    <TextBox x:Name="txtCantidad200"/>
                    <TextBox x:Name="txtCantidad100" Margin="0 8"/>
                    <TextBox x:Name="txtCantidad50"/>
                    <TextBox x:Name="txtCantidad20" Margin="0 8"/>
                    <TextBox x:Name="txtCantidad10"/>
                    <Label Content="Total" HorizontalAlignment="Center" Name="lblTotal" Margin="0 8"/>
                    <Button x:Name="btnGuadar1" Content="Guardar" Margin="0 20" />

                </StackPanel>
                <StackPanel Orientation="Vertical" Width="121">
                    <Label Content="Importe" HorizontalAlignment="Center"/>
                    <TextBox x:Name="txtSuma1000" Margin="10 4 0 0"/>
                    <TextBox x:Name="txtSuma500" Margin="10 8 0 8"/>
                    <TextBox x:Name="txtSuma200" Margin="10 0 0 0"/>
                    <TextBox x:Name="txtSuma100" Margin="10 8 0 8"/>
                    <TextBox x:Name="txtSuma50" Margin="10 0 0 0"/>
                    <TextBox x:Name="txtSuma20" Margin="10 8 0 8"/>
                    <TextBox x:Name="txtSuma10" Margin="10 0 0 0"/>
                    <TextBox x:Name="txtSumaTotal" Margin="10 12 0 8"/>
                    <Button x:Name="btnLimpiar1"  Width="80" Content="Limpiar" Margin="0 24 0 0" />
